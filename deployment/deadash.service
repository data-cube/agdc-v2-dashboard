[Unit]
Description=Gunicorn service for DEA Dashboard
Requires=nginx.service

[Service]
User=deadash
Group=deadash
WorkingDirectory=/var/www/dea-dashboard
ExecStart=/opt/conda/bin/gunicorn -b '127.0.0.1:8000' -w 6 --timeout 900 cubedash:app
PrivateTmp=true
KillMode=process
Restart=on-failure
RestartSec=42s

